ildren":[{"Type":"NodeText","Data":"150.75"}]}]},{"Type":"NodeTableRow","Data":"tr","Children":[{"Type":"NodeTableCell","Data":"td","Children":[{"Type":"NodeText","Data":"秒表计时20次"}]},{"Type":"NodeTableCell","Data":"td","Children":[{"Type":"NodeText","Data":"150.32"}]},{"Type":"NodeTableCell","Data":"td","Children":[{"Type":"NodeText","Data":"149.56"}]},{"Type":"NodeTableCell","Data":"td","Children":[{"Type":"NodeText","Data":"150.29"}]},{"Type":"NodeTableCell","Data":"td","Children":[{"Type":"NodeText","Data":"150.42"}]},{"Type":"NodeTableCell","Data":"td","Children":[{"Type":"NodeText","Data":"150.30"}]}]}]},{"ID":"20241009233326-ek9eu5t","Type":"NodeParagraph","Properties":{"id":"20241009233326-ek9eu5t","updated":"20241009233326"},"Children":[{"Type":"NodeText","Data":"Claude的数据处理"}]},{"ID":"20241009233326-qjc1a5d","Type":"NodeCodeBlock","IsFencedCodeBlock":true,"Properties":{"id":"20241009233326-qjc1a5d","updated":"20241009233326"},"Children":[{"Type":"NodeCodeBlockFenceOpenMarker","Data":"```"},{"Type":"NodeCodeBlockFenceInfoMarker","CodeBlockInfo":"cHl0aG9u"},{"Type":"NodeCodeBlockCode","Data":"import numpy as np\n\n# 常量定义\nm = 558.0 / 1000  # 圆环质量（kg）\nR = 0.200 / 1000  # 钢丝半径（m）\nG_accepted = 7.8e10  # G的公认值（N/m^2）\nL = 73.2 / 100  # 铁丝长度（m）\n\n# 数据输入\ninner_diameters = np.array([79.9, 79.92, 79.70, 79.80, 80.00]) / 1000  # 转换为米\nouter_diameters = np.array([110.10, 110.18, 110.22, 110.12, 110.20]) / 1000  # 转换为米\n\nT0_hall = np.array([57.48, 58.24, 59.06, 59.20, 59.10]) / 10  # 除以10得到单个周期\nT0_stopwatch = np.array([57.51, 55.11, 54.11, 57.19, 54.59]) / 10\n\nT1_hall = np.array([151.27, 150.77, 150.99, 150.95, 150.75]) / 10\nT1_stopwatch = np.array([150.32, 149.56, 150.29, 150.42, 150.30]) / 10\n\n# 计算平均值和标准差\nR1_avg = np.mean(inner_diameters) / 2\nR2_avg = np.mean(outer_diameters) / 2\nT0_hall_avg = np.mean(T0_hall)\nT0_stopwatch_avg = np.mean(T0_stopwatch)\nT1_hall_avg = np.mean(T1_hall)\nT1_stopwatch_avg = np.mean(T1_stopwatch)\n\n# 计算I1\nI1 = m * (R1_avg**2 + R2_avg**2) / 2\n\n# 计算G（使用霍尔传感器数据）\nG_hall = (8 * np.pi * L) / (R**4) * (I1 / (T1_hall_avg**2 - T0_hall_avg**2))\n\n# 计算G（使用秒表数据）\nG_stopwatch = (8 * np.pi * L) / (R**4) * (I1 / (T1_stopwatch_avg**2 - T0_stopwatch_avg**2))\n\n# 计算相对误差\nE0_hall = abs(G_accepted - G_hall) / G_accepted * 100\nE0_stopwatch = abs(G_accepted - G_stopwatch) / G_accepted * 100\n\n# 计算标准差\nT0_hall_std = np.std(T0_hall, ddof=1)\nT0_stopwatch_std = np.std(T0_stopwatch, ddof=1)\nT1_hall_std = np.std(T1_hall, ddof=1)\nT1_stopwatch_std = np.std(T1_stopwatch, ddof=1)\n\n# 计算不确定度\ndef calculate_uncertainty(G, I1, T1, T0, dI1, dT1, dT0, L, R):\n    dG_I1 = G / I1 * dI1\n    dG_T1 = 2 * G * T1 / (T1**2 - T0**2) * dT1\n    dG_T0 = 2 * G * T0 / (T1**2 - T0**2) * dT0\n    dG_L = G / L * 0.001  # 假设长度测量误差为1mm\n    dG_R = 4 * G / R * 0.00001  # 假设半径测量误差为0.01mm\n\n    return np.sqrt(dG_I1**2 + dG_T1**2 + dG_T0**2 + dG_L**2 + dG_R**2)\n\n# 计算I1的不确定度\ndR1 = np.std(inner_diameters, ddof=1) / 2\ndR2 = np.std(outer_diameters, ddof=1) / 2\ndI1 = m * np.sqrt((R1_avg * dR1)**2 + (R2_avg * dR2)**2)\n\n# 计算G的不确定度\ndG_hall = calculate_uncertainty(G_hall, I1, T1_hall_avg, T0_hall_avg, dI1, T1_hall_std, T0_hall_std, L, R)\ndG_stopwatch = calculate_uncertainty(G_stopwatch, I1, T1_stopwatch_avg, T0_stopwatch_avg, dI1, T1_stopwatch_std, T0_stopwatch_std, L, R)\n\n# 打印结果\nprint(f\"平均内径: {2*R1_avg*1000:.2f} ± {2*dR1*1000:.2f} mm\")\nprint(f\"平均外径: {2*R2_avg*1000:.2f} ± {2*dR2*1000:.2f} mm\")\nprint(f\"霍尔传感器测量的G值: {G_hall/1e9:.2f} ± {dG_hall/1e9:.2f} GPa\")\nprint(f\"秒表测量的G值: {G_stopwatch/1e9:.2f} ± {dG_stopwatch/1e9:.2f} GPa\")\nprint(f\"霍尔传感器测量的相对误差: {E0_hall:.2f}%\")\nprint(f\"秒表测量的相对误差: {E0_stopwatch:.2f}%\")\n\nprint(f\"\\\\n标准差:\")\nprint(f\"T0 (霍尔): {T0_hall_std:.4f} s\")\nprint(f\"T0 (秒表): {T0_stopwatch_std:.4f} s\")\nprint(f\"T1 (霍尔): {T1_hall_std:.4f} s\")\nprint(f\"T1 (秒表): {T1_stopwatch_std:.4f} s\")\n"},{"Type":"NodeCodeBlockFenceCloseMarker","Data":"```"}]},{"ID":"20241009233326-1sik7sa","Type":"NodeBlockquo