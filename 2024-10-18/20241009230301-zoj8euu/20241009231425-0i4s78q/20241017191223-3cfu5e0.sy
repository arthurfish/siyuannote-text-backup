 7.4, 3.6, 1.6, 0.7, 0.3, 1.1, 0.6, 0.2],\n    [99.9, 99.6, 99.0, 96.2, 87.1, 80.5, 72.3, 63.4, 54.9, 46.3, 38.2, 31.1, 24.9, 15.7, 9.6, 6.0, 4.0, 3.1, 2.7, 3.4, 3.0, 2.6],\n    [99.9, 99.6, 99.0, 96.1, 87.1, 80.5, 72.2, 63.5, 54.9, 46.2, 38.2, 31.2, 25.0, 15.8, 9.7, 6.0, 4.0, 3.1, 2.8, 3.5, 3.0, 2.6]\n])\n\n# 计算平均透光率和标准差\nmean_transmittance = np.mean(transmittance, axis=0)\nstd_transmittance = np.std(transmittance, axis=0)\n\n# 创建图形\nplt.figure(figsize=(12, 8))\n\n# 绘制平均透光率曲线\nplt.plot(voltages, mean_transmittance, 'b-', label='avg')\n\n# 绘制误差范围\nplt.fill_between(voltages, mean_transmittance - std_transmittance,\n                 mean_transmittance + std_transmittance,\n                 alpha=0.2, color='b', label='std range')\n\n# 绘制原始数据点\nfor i in range(4):\n    plt.plot(voltages, transmittance[i], 'o', markersize=3, alpha=0.5, label=f'dataset {i+1}')\n\n# 设置图表属性\nplt.xlabel('Voltage (V)')\nplt.ylabel('light transmittance (%)')\nplt.title('graph')\nplt.xscale('log')  # 使用对数刻度以更好地显示低电压区域\nplt.grid(True, which=\"both\", ls=\"-\", alpha=0.2)\nplt.legend()\n\n# 显示图表\nplt.tight_layout()\nplt.show()\n\n# 打印一些统计数据\nprint(\"电压值对应的平均透光率：\")\nfor v, t in zip(voltages, mean_transmittance):\n    print(f\"电压 {v:.2f}V: {t:.2f}%\")\n\nprint(\"\\n最大透光率:\", np.max(mean_transmittance))\nprint(\"最小透光率:\", np.min(mean_transmittance))\nprint(\"透光率变化最大的电压范围:\")\nmax_change_index = np.argmax(np.abs(np.diff(mean_transmittance)))\nprint(f\"{voltages[max_change_index]:.2f}V 到 {voltages[max_change_index+1]:.2f}V\")\n"},{"Type":"NodeCodeBlockFenceCloseMarker","Data":"```"}]},{"ID":"20241017191229-psskzjs","Type":"NodeBlockquote","Properties":{"id":"20241017191229-psskzjs","updated":"20241017191229"},"Children":[{"Type":"NodeBlockquoteMarker","Data":"\u003e"},{"ID":"20241017191229-vu3fdit","Type":"NodeParagraph","Properties":{"id":"20241017191229-vu3fdit","updated":"20241017191229"},"Children":[{"Type":"NodeText","Data":"…最大透光率: 99.92500000000001\n最小透光率: 1.4\n透光率变化最大的电压范围:\n1.50V 到 1.60V"}]}]},{"ID":"20241017191229-0qaq6sd","Type":"NodeParagraph","Properties":{"id":"20241017191229-0qaq6sd","updated":"20241017191229"},"Children":[{"Type":"NodeText","Data":"​"},{"Type":"NodeImage","Data":"span","Children":[{"Type":"NodeBang"},{"Type":"NodeOpenBracket"},{"Type":"NodeLinkText","Data":"image 1.png"},{"Type":"NodeCloseBracket"},{"Type":"NodeOpenParen"},{"Type":"NodeLinkDest","Data":"assets/notion/27/278ae6207f686cfe0aecc9c49ba3ea56.png"},{"Type":"NodeCloseParen"}]},{"Type":"NodeText","Data":"​"}]},{"ID":"20241017191229-pv2m9vx","Type":"NodeHeading","HeadingLevel":1,"Properties":{"id":"20241017191229-pv2m9vx","updated":"20241017191229"},"Children":[{"Type":"NodeText","Data":"实验内容及操作指导"}]},{"ID":"20241017191229-3sndf3v","Type":"NodeList","ListData":{"Typ":1},"Properties":{"id":"20241017191229-3sndf3v","updated":"20241017191229"},"Children":[{"ID":"20241017191229-vzk47bp","Type":"NodeListItem","ListData":{"Typ":1,"Delimiter":46,"Marker":"MS4=","Num":1},"Properties":{"id":"20241017191229-vzk47bp","updated":"20241017191229"},"Children":[{"ID":"20241017191229-lb2aeby","Type":"NodeParagraph","Properties":{"id":"20241017191229-lb2aeby","updated":"20241017191229"},"Children":[{"Type":"NodeText","Data":"接通电源，检查模式转换开关是否处于静态状态，然后打开光电及液晶箱后面电源开关，将看到光源发出白光。使激光器预热10分钟左右。在预热过程中，轻轻转动液晶转盘，使其0刻度与刻度红对齐，将观察到电压示值000。透过率显示值应在600左右。"}]}]},{"ID":"20241017191229-1dfh4u0","Type":"NodeListItem","ListData":{"Typ":1,"Delimiter":46,"Marker":"Mi4=","Num":2},"Properties":{"id":"20241017191229-1dfh4u0","updated":"20241017191229"},"Children":[{"ID":"20241017191229-tiulw3x","Type":"NodeParagraph","Properties":{"id":"20241017191229-tiulw3x","updated":"20241017191229"},"Children":[{"Type":"NodeText","Data":"测量液晶光开关的电光特性待透过率显示示值稳定时，按下透过率校准按钮3秒，将看到透过率显示变为100。"}]}]}]},{"ID":"20241017191229-zxm41zp","Type":"NodeParagraph","Properties":{"id":"20241017191229-zxm41zp","updated":"20241017191229"},"Children":[{"Type":"NodeText","Data":"然后按电压调节按钮以改变液晶供电电压，使得电压值从0V到5V变化，记录相应电压下的透射率数值，然后将电压降到0，再次重复测量。"}]},{"ID":"20241017191229-s08mnwx","Type":"NodeParagraph","Properties":{"id":"20241017191229-s08mnwx","updated":"20241017191229"},"Children":[{"Type":"NodeText","Data":"重复3次并计算相应电压下透射率的平均值。"}]},{"ID":"20241017191229-qd9a14g","Type":"NodeParagraph","Properties":{"id":"20241017191229-qd9a14g","updated":"20241017191229"},"Children":[{"Type":"NodeText","Data":"注：第二、三次测量，将电压从5V降到0V时，如果透过率小于100%，可按下校准按钮，回到600左右，再次重新校准透过率，然后测量。"}]},{"ID":"20241017191229-e21qg16","Type":"NodeParagraph","Properties":{"id":"20241017191229-e21qg16","updated":"20241017191229"},"Children":[{"Type":"NodeText","Data":"依据实验数据绘制电光特性曲线，求出阈值电压和关断电压，求两个电压值。"}]},{"ID":"20241017191229-gcaj5l7","Type":"NodeList","ListData":{"Typ":1},"Properties":{"id":"20241017191229-gcaj5l7","updated":"20241017191229"},"Children":[{"ID":"20241017191229-jgpv4yy","Type":"NodeListItem","ListData":{"Typ":1,"Delimiter":46,"Marker":"MS4=","Num":1},"Properties":{"id":"20241017191229-jgpv4yy","updated":"20241017191229"},"Children":[{"ID":"20241017191229-bfanivu","Type":"NodeParagraph","Properties":{"id":"20241017191229-bfanivu","updated":"20241017191229"},"Children":[{"Type":"NodeText","Data":"测量液晶的时间响应特性将液晶供电电压调到2.00V，按一下开关矩阵右上角清屏/闪烁按钮，将看到液晶屏闪烁，透过率显示数值也在变化。打开数字存储示波器电源，观察在液晶静态闪烁状态下光开关的时间响应特性曲线，利用示波器的功能测量液晶的上升时间Δt1和下降时间Δt2。重复三次测量。"}]},{"ID":"20241017191229-id9k5kv","Type":"NodeParagraph","Properties":{"id":"20241017191229-id9k5kv","updated":"20241017191229"},"Children":[{"Type":"NodeText","Data":"注：示波器X轴刻度位于250ms/格，Y1为1V/格，Y2为2V/格。"}]}]},{"ID":"20241017191229-kz4jx89","Type":"NodeListItem","ListData":{"Typ":1,"Delimiter":46,"Marker":"Mi4=","Num":2},"Properties":{"id":"20241017191229-kz4jx89","updated":"20241017191229"},"Children":[{"ID":"20241017191229-cyfcc9o","Type":"NodeParagraph","Properties":{"id":"20241017191229-cyfcc9o","updated":"20241017191229"},"Children":[{"Type":"NodeText","Data":"演示液晶显示器的显示原理按下模式转换开关，使液晶屏处于动态模式。并将液晶供电电压调到4.5V左右。"}]},{"ID":"20241017191229-ovmmwxi","Type":"NodeParagraph","Properties":{"id":"20241017191229-ovmmwxi","updated":"20241017191229"},"Children":[{"Type":"NodeText","Data":"初始时各相素都处于开通状态。按1次矩阵开关板上的某一按键，可改变相应液晶相素的通断状态。按矩阵上的按键组合成一个字符或文字。矩阵开关板右上角的按键为清屏键，用以清除已输入在显示屏上的图形。"}]}]},{"ID":"20241017191229-s9hr7n6","Type":"NodeListItem","ListData":{"Typ":1,"Delimiter":46,"Marker":"My4=","Num":3},"Properties":{"id":"20241017191229-s9hr7n6","updated":"20241017191229"},"Children":[{"ID":"20241017191229-72lt5ho","Type":"NodeParagraph","Properties":{"id":"20241017191229-72lt5ho","updated":"20241017191229"},"Children":[{"Type":"NodeText","Data":"液晶光开关的视角特性（选做）将模式转换开关置于静态模式，首先将透过率显示调须到100，然后再进行实验。"}]},{"ID":"20241017191229-59gezeu","Type":"NodeParagraph","Properties":{"id":"20241017191229-59gezeu","updated":"20241017191229"},"Children":[{"Type":"NodeText","Data":"在供电电压为0V时，调节液晶屏与入射激光的角度，测量光强透过率最大值TMAX。然后将供电电压置于2V，再次调节液晶屏角度，测量光强透过率最小值TMIN，并计算其对比度。以角度为横坐标，对比度为纵坐标，绘制水平方向对比度随入射光入射角而变化的曲线。"}]}]}]}]}